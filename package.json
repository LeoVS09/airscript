{
  "name": "airscript",
  "version": "0.1.0",
  "description": "Simple js dialect",
  "main": "index.js",
  "scripts": {
    "test": "babel-node --extensions \".js\",\".ts\" test/index.js --expose-debug-as=v8debug",
    "test:watch": "babel-watch --extensions \".js\",\".ts\" test/index.js",
    "clean": "del /s /q build",
    "build": "babel src -d dist",
    "compile": "babel-node --extensions \".js\",\".ts\" compile.ts",
    "run-build": "babel-node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoVS09/airscript.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeoVS09/airscript/issues"
  },
  "homepage": "https://github.com/LeoVS09/airscript#readme",
  "dependencies": {
    "make-dir": "^1.3.0"
  },
  "devDependencies": {
    "lodash": "^4.17.11",
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@types/node": "^10.12.15",
    "babel-watch": "https://github.com/monathur/babel-watch.git",
    "typescript": "^3.2.2"
  }
}
